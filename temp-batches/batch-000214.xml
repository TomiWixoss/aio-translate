    <TextStringDefinition InstanceID="0xC0F48598" TextString="Ra ngoài, có thể vui đấy!" />
    <TextStringDefinition InstanceID="0x03F8BEAD" TextString="Chơi game; lướt web." />
    <TextStringDefinition InstanceID="0x36F6F3A8" TextString="Khám phá khía cạnh sáng tạo của tôi." />
    <TextStringDefinition InstanceID="0x532DDE4B" TextString="Quây quần với bạn bè." />
    <TextStringDefinition InstanceID="0x63BA2001" TextString="&lt;font color=&quot;#29AF4A&quot;&gt;◴ Sở thích&lt;/font&gt;" />
    <TextStringDefinition InstanceID="0xACE07389" TextString="&lt;font color=&quot;#EC262E&quot;&gt;◵ Không thích&lt;/font&gt;" />
    <TextStringDefinition InstanceID="0xF49634A8" TextString="Sim vui hơn khi nghe nhạc mà họ Thích và cảm thấy bất an khi nghe nhạc mà họ Không thích. Sim cũng có thể chia sẻ ý kiến âm nhạc của mình với Sim khác!" />
    <TextStringDefinition InstanceID="0xBF8A74AC" TextString="Âm nhạc" />
    <TextStringDefinition InstanceID="0x3E211BA7" TextString="Thể loại nhạc" />
    <TextStringDefinition InstanceID="0x37D2B98D" TextString="Màu sắc" />
    <TextStringDefinition InstanceID="0x49FC6CEA" TextString="Mọi người cần một Màu sắc yêu thích! Sim có thể Hỏi Sim khác về Màu sắc yêu thích của họ, và Sim trẻ có thể đi xa hơn." />
    <TextStringDefinition InstanceID="0x9AA5C01E" TextString="Màu sắc" />
    <TextStringDefinition InstanceID="0x16EFAF43" TextString="Sở thích &amp; Kỹ năng" />
    <TextStringDefinition InstanceID="0x89829714" TextString="Sim vui hơn và có nhiều niềm vui hơn khi họ làm Sở thích &amp; Kỹ năng mà họ Thích. Mặt khác, làm Sở thích &amp; Kỹ năng mà họ Không thích sẽ khiến Sim căng thẳng và cạn kiệt niềm vui." />
    <TextStringDefinition InstanceID="0xECD72B98" TextString="Hoạt động" />
    <TextStringDefinition InstanceID="0x7E750D0F" TextString="Chi tiết miệng" />
    <TextStringDefinition InstanceID="0xC1A215C4" TextString="Chi tiết mắt" />
    <TextStringDefinition InstanceID="0x2B3C806F" TextString="Chi tiết má" />
    <TextStringDefinition InstanceID="0xCBC05467" TextString="Chi tiết lông mày" />
    <TextStringDefinition InstanceID="0x96B41ECC" TextString="Tóc phía sau" />
    <TextStringDefinition InstanceID="0xDFDE4EE6" TextString="Chi tiết da" />
    <TextStringDefinition InstanceID="0x1655F56F" TextString="Sẹo trên cơ thể" />
    <TextStringDefinition InstanceID="0x2A019D65" TextString="Móng chân" />
    <TextStringDefinition InstanceID="0xB5C4D541" TextString="Quần áo thú cưng" />
    <TextStringDefinition InstanceID="0xB25FD8C0" TextString="Thiết bị y tế đeo" />
    <TextStringDefinition InstanceID="0x5B7A5F8C" TextString="Thiết bị y tế đeo" />
    <TextStringDefinition InstanceID="0x71DEC7B7" TextString="Móng tay" />
    <TextStringDefinition InstanceID="0x0745549B" TextString="Tóc thân người" />
    <TextStringDefinition InstanceID="0xA62A99EE" TextString="Tóc chân" />
    <TextStringDefinition InstanceID="0x852DEC10" TextString="Tóc tay" />
    <TextStringDefinition InstanceID="0xAA78E092" TextString="Tham gia" />
    <TextStringDefinition InstanceID="0x590CB7FC" TextString="Gửi một mình" />
    <TextStringDefinition InstanceID="0x5A329D39" TextString="Nhà báo" />
    <TextStringDefinition InstanceID="0x6AE92763" TextString="Nhà báo" />
    <TextStringDefinition InstanceID="0x6EA9322C" TextString="Từ ngữ có thể là vũ khí mạnh mẽ, khi được dùng để phơi bày sự thật. Với tư cách là Nhà báo, bạn sẽ khai thác những tin tức quan trọng và rải chúng cho mọi người thấy.\nPhần thưởng bao gồm khả năng phỏng vấn các Sim khác về cuộc sống của họ." />
    <TextStringDefinition InstanceID="0x51B9FB50" TextString="Tác giả" />
    <TextStringDefinition InstanceID="0xFD1AC748" TextString="Tác giả" />
    <TextStringDefinition InstanceID="0xB9C8245D" TextString="Nếu bạn yêu sự tự do để trang trí và sáng tạo, cuộc sống của Tác giả dành cho bạn. Gọi ra một nhân vật, đuổi theo một câu chuyện.... Từ viết là của bạn.\nPhần thưởng bao gồm khả năng kiếm được nhiều tiền hơn từ sách." />
    <TextStringDefinition InstanceID="0x79BC4807" TextString="Người viết" />
    <TextStringDefinition InstanceID="0x6F1D4C69" TextString="Người viết" />
    <TextStringDefinition InstanceID="0xA1455C36" TextString="Được trả tiền để thể hiện ý tưởng của bạn với những cách diễn đạt đẹp, và từ ngữ—những từ ngữ tuyệt vời!" />
    <TextStringDefinition InstanceID="0x77D1D57E" TextString="Sim này sẽ cần đi làm giữa chừng Sự kiện Xã hội này." />
    <TextStringDefinition InstanceID="0x3DDBEF02" TextString="Nhân viên bán lẻ" />
    <TextStringDefinition InstanceID="0x7E3538D2" TextString="Nhân viên bán lẻ" />
    <TextStringDefinition InstanceID="0x9222A483" TextString="Hướng tới tương lai bằng cách bắt đầu sự nghiệp bán hàng. Từ tạo ra trưng bày đến làm hài lòng khách hàng, bạn đang trên đường đến những phúc lợi, giảm giá và những Simoleon ngọt ngào." />
    <TextStringDefinition InstanceID="0xC64CD9D7" TextString="Người lao động phổ thông" />
    <TextStringDefinition InstanceID="0x74827D19" TextString="Người lao động phổ thông" />
    <TextStringDefinition InstanceID="0x7EC62686" TextString="Bạn trẻ và khỏe mạnh, vì vậy hãy nhận tiền từ những người không phải vậy. Từ cắt cỏ đến vận hành máy móc lớn, cơ bắp của bạn là nơi kiếm tiền." />
    <TextStringDefinition InstanceID="0x38DFE93F" TextString="Nhân viên đồ ăn nhanh" />
    <TextStringDefinition InstanceID="0xE86A13A1" TextString="Nhân viên đồ ăn nhanh" />
