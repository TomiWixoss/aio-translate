    <TextStringDefinition InstanceID="0x573AA7C6" TextString="Trang Trí Vui Vẻ" />
    <TextStringDefinition InstanceID="0xAB450F24" TextString="Bị Đồng Nghiệp Hét Lên" />
    <TextStringDefinition InstanceID="0x450655D1" TextString="Có lẽ trung thực không phải là chính sách tốt nhất sau tất cả." />
    <TextStringDefinition InstanceID="0x2883B4E7" TextString="Những khoảnh khắc phép thuật với bạn bè nhồi bông khiến Sim cảm thấy được truyền cảm hứng." />
    <TextStringDefinition InstanceID="0x2F2411A6" TextString="Bạn Thân Lớn" />
    <TextStringDefinition InstanceID="0x85C60C0C" TextString="Gần Không Thân Thiện" />
    <TextStringDefinition InstanceID="0x812FAC0B" TextString="Quyền Riêng Tư Bị Xâm Phạm!" />
    <TextStringDefinition InstanceID="0x35F92C44" TextString="Sim không thích khi ai đó bất ngờ bước vào phòng họ!" />
    <TextStringDefinition InstanceID="0xF94132AD" TextString="Không Thể Không Nhìn Thấy!" />
    <TextStringDefinition InstanceID="0x782D2C92" TextString="Bạn đã bất ngờ bắt gặp thứ bạn không bao giờ nên thấy." />
    <TextStringDefinition InstanceID="0x7D403D41" TextString="Tin Đồn Thú Vị" />
    <TextStringDefinition InstanceID="0x712882BE" TextString="Vừa nghe một tin đồn phản cảm và nóng bỏng!" />
    <TextStringDefinition InstanceID="0x2A5B616A" TextString="Nhà Đầu Tư Dũng Cảm" />
    <TextStringDefinition InstanceID="0x6E873A6B" TextString="Cảm thấy tự tin sau khi học một vài mẹo tinh vi về đầu tư." />
    <TextStringDefinition InstanceID="0x5D06DBBE" TextString="Đánh Lừa Tôi Một Lần" />
    <TextStringDefinition InstanceID="0x5789D007" TextString="Thật khó chịu khi ai đó cố gắng lừa dối bạn." />
    <TextStringDefinition InstanceID="0x5999E1DE" TextString="Cảm Thấy Quan Trọng" />
    <TextStringDefinition InstanceID="0xF50FC2A7" TextString="Không gì như ra lệnh cho người khác để xây dựng lòng tự tin!" />
    <TextStringDefinition InstanceID="0x99D887AB" TextString="Bẩn Thỉu" />
    <TextStringDefinition InstanceID="0x1BF83164" TextString="Chắc chắn có sự tích tụ bụi bẩn đang xảy ra." />
    <TextStringDefinition InstanceID="0xE0F51723" TextString="Học Được Điều Mới" />
    <TextStringDefinition InstanceID="0xFCA962EC" TextString="Ôi! Một kỹ năng mới có nghĩa là nhiều điều tuyệt vời chắc chắn sẽ đến." />
    <TextStringDefinition InstanceID="0x6A3240EB" TextString="Một Mình" />
    <TextStringDefinition InstanceID="0xAE02CFA4" TextString="Đi ra ngoài một mình và vui vẻ làm mọi thứ mà không cần giúp đỡ." />
    <TextStringDefinition InstanceID="0x99281395" TextString="Mông Mông Mông" />
    <TextStringDefinition InstanceID="0xCDB6838A" TextString="Bạn biết gì không? Mông gà! Haha, điều đó chưa bao giờ cũ!" />
    <TextStringDefinition InstanceID="0x5F3C223E" TextString="Dưới Ngón Tay Tôi" />
    <TextStringDefinition InstanceID="0x0F9BEF87" TextString="Ai đó đã chú ý đến cơn bùng nổ hoàn toàn &quot;hợp lý&quot; của tôi. Ai nói cơn giận dữ không hữu ích?" />
    <TextStringDefinition InstanceID="0x1F683FFE" TextString="Căng Thẳng Trẻ Mới Biết Đi" />
    <TextStringDefinition InstanceID="0xBF8D1847" TextString="{0.SimFirstName} đang thực sự căng thẳng vì tính khó chiều của đứa trẻ mới biết đi này. Ai mới là người đứng đầu chứ?" />
    <TextStringDefinition InstanceID="0xF44323BD" TextString="Sự Bực Bội Vì Tính Khó Chiều" />
    <TextStringDefinition InstanceID="0x250ABDC2" TextString="Ái chà! Đôi khi trẻ mới biết đi có thể thật đáng bực! {0.SimFirstName} không thể chờ đến giờ ngủ trưa." />
