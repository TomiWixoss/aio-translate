    <TextStringDefinition InstanceID="0xDE1308BA" TextString="Chất lượng Thức ăn:" />
    <TextStringDefinition InstanceID="0x9AAB309A" TextString="Giảm Khó chịu:" />
    <TextStringDefinition InstanceID="0x8ED8623F" TextString="+Kỹ năng WooHoo" />
    <TextStringDefinition InstanceID="0x8386D7CC" TextString="+Kỹ năng Viết" />
    <TextStringDefinition InstanceID="0xBC92BF7B" TextString="+Kỹ năng Khoa học Tên lửa" />
    <TextStringDefinition InstanceID="0x4E1BEAA0" TextString="Môi trường:" />
    <TextStringDefinition InstanceID="0xF0177D76" TextString="Giảm Căng thẳng:" />
    <TextStringDefinition InstanceID="0x9921F1E7" TextString="Vui vẻ:" />
    <TextStringDefinition InstanceID="0x916FFA5F" TextString="+Kỹ năng Thể dục" />
    <TextStringDefinition InstanceID="0x25C0D591" TextString="+Kỹ năng Vẽ tranh" />
    <TextStringDefinition InstanceID="0x496507AC" TextString="+Kỹ năng Lập luận" />
    <TextStringDefinition InstanceID="0x3F450257" TextString="Bất khả chiến bại" />
    <TextStringDefinition InstanceID="0xCA879377" TextString="+Kỹ năng Lập trình" />
    <TextStringDefinition InstanceID="0x3E2B755B" TextString="Chất lượng Tranh:" />
    <TextStringDefinition InstanceID="0x7D430A6D" TextString="Đói:" />
    <TextStringDefinition InstanceID="0x159D8395" TextString="Trang trí Trầm uất:" />
    <TextStringDefinition InstanceID="0x2283BC3B" TextString="Cảm thấy Đầy đủ" />
    <TextStringDefinition InstanceID="0xCF7A7654" TextString="{0.SimFirstName} đánh dấu một nhiệm vụ nữa trong danh sách những điều {M0.HE}{F0.SHE} muốn làm." />
    <TextStringDefinition InstanceID="0x0B775BED" TextString="Đau lưng!" />
    <TextStringDefinition InstanceID="0xF6832AD2" TextString="Ái chà! Chiếc nệm đó cứng quá. Như bê tông vậy." />
    <TextStringDefinition InstanceID="0xAF3AA2A9" TextString="Voodoo: Bất an" />
    <TextStringDefinition InstanceID="0xEF19C476" TextString="Bạn có một cảm giác kỳ lạ, bạn cảm thấy bất an và không giống như chính mình. Điều gì đã xảy ra? Chuyện gì đang diễn ra?" />
    <TextStringDefinition InstanceID="0x03766EE4" TextString="Voodoo: Đau đớn" />
    <TextStringDefinition InstanceID="0x880F2691" TextString="Ái chà! Nó giống như cảm giác ‘kim châm’ khi bàn chân ngủ, nhưng khắp người, ngẫu nhiên, với những que kim nóng trắng." />
    <TextStringDefinition InstanceID="0x63672990" TextString="Voodoo: Thích tán tỉnh" />
    <TextStringDefinition InstanceID="0x294119F5" TextString="Có những lúc bạn cảm thấy “thèm khát” hơn những lúc khác, và rồi có cảm giác bùng nổ đột ngột mà bạn đang cảm nhận ngay bây giờ." />
    <TextStringDefinition InstanceID="0xD8DBAF4D" TextString="Voodoo: Vui sướng" />
    <TextStringDefinition InstanceID="0x0762FEB2" TextString="Bạn đột nhiên tràn ngập hạnh phúc, như thể bạn có thể bùng nổ thành chuyến bay tự nhiên!" />
    <TextStringDefinition InstanceID="0xFD716162" TextString="Voodoo: Choáng váng" />
    <TextStringDefinition InstanceID="0x7616B833" TextString="{0.SimFirstName} cảm thấy lạ lùng Choáng váng, như thể {M0.he}{F0.she} đã đập vào {M0.his}{F0.her} đầu." />
    <TextStringDefinition InstanceID="0xEDE30CEF" TextString="Bạn Làm Voodoo" />
    <TextStringDefinition InstanceID="0x793128EC" TextString="Tự tin nhờ biết rằng số phận của một Sim khác giờ đã nằm trong tay {0.SimFirstName}." />
    <TextStringDefinition InstanceID="0x72A69178" TextString="Voodoo: Thất bại" />
    <TextStringDefinition InstanceID="0xC5CCACAD" TextString="Nỗ lực của {0.SimFirstName} để làm sáng ngày của ai đó bằng phép thuật đã thất bại và khiến {M0.him}{F0.her} thất vọng." />
    <TextStringDefinition InstanceID="0x990DDC27" TextString="Voodoo: Phản đòn" />
    <TextStringDefinition InstanceID="0x16287E88" TextString="Sức mạnh của nghiệp báo buộc bạn phải gặt hái những gì bạn đã gieo." />
    <TextStringDefinition InstanceID="0x89321E95" TextString="Giai điệu U ám" />
    <TextStringDefinition InstanceID="0x6A834C8A" TextString="Buồn bã vì những nốt nhạc Đau lòng của Đàn violin" />
    <TextStringDefinition InstanceID="0x3D0863BF" TextString="Buồn bã  vì Nghệ thuật" />
    <TextStringDefinition InstanceID="0x34ADF590" TextString="Không ai có thể phủ nhận sức mạnh của nghệ thuật lớn lao đến mức khiến nước mắt rơi." />
    <TextStringDefinition InstanceID="0x97EEAD2E" TextString="Cười khúc khích vì Nghệ thuật" />
    <TextStringDefinition InstanceID="0xD03CC057" TextString="Thế giới đầy những điều hài hước, và nghệ thuật cũng không ngoại lệ." />
    <TextStringDefinition InstanceID="0xE9AA1B3D" TextString="Cảm hứng từ Nghệ thuật" />
    <TextStringDefinition InstanceID="0xEF716842" TextString="Có gì truyền cảm hứng hơn là ngắm nhìn những kiệt tác nghệ thuật?" />
    <TextStringDefinition InstanceID="0x0D0CC5B1" TextString="Vui lên vì Nghệ thuật" />
    <TextStringDefinition InstanceID="0x34B5918E" TextString="Nghệ thuật đẹp đẽ, kích thích đủ để lấp đầy một người với niềm vui." />
    <TextStringDefinition InstanceID="0x8D1C324E" TextString="Tĩnh tâm nhờ Nghệ thuật" />
    <TextStringDefinition InstanceID="0x22F2E1F7" TextString="Nhìn đúng tác phẩm nghệ thuật đủ lâu và bất kỳ ai cũng có thể tìm thấy trung tâm của mình." />
    <TextStringDefinition InstanceID="0x694D9A77" TextString="Cảm hứng Lãng mạn" />
    <TextStringDefinition InstanceID="0xD85BE678" TextString="Ngắm nhìn một số nghệ thuật có thể đặt bạn vào tâm trạng!" />
