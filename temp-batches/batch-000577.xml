    <TextStringDefinition InstanceID="0xA1CC50AC" TextString="Bị nhục nhã sau khi liếc thấy mình trong gương ở một góc độ không may!" />
    <TextStringDefinition InstanceID="0xCB5DBA83" TextString="Cảm thấy lôi thôi" />
    <TextStringDefinition InstanceID="0xDA02C7CA" TextString="Bị nhục nhã từ các Sự kiện Xã hội gần đây!" />
    <TextStringDefinition InstanceID="0xE9D91D51" TextString="Xấu hổ" />
    <TextStringDefinition InstanceID="0xCC92B797" TextString="Một trận đánh đấm dữ dội có thể khiến một Sim cảm thấy hơi choáng váng." />
    <TextStringDefinition InstanceID="0x53018F46" TextString="Tốt (Luôn bật và ẩn mặc định)" />
    <TextStringDefinition InstanceID="0xE55ECA29" TextString="Cái gì vậy?" />
    <TextStringDefinition InstanceID="0x203E7EF6" TextString="Đọc riêng lẻ từ ngữ thì có nghĩa, nhưng kết hợp lại... cái gì? Cuốn sách này vượt quá khả năng của tôi." />
    <TextStringDefinition InstanceID="0xBC1127C3" TextString="Cảm thấy hơi chán khi đọc một cuốn sách ở trình độ kỹ năng thấp." />
    <TextStringDefinition InstanceID="0x552370AB" TextString="Câu chuyện bi thảm" />
    <TextStringDefinition InstanceID="0x0726A464" TextString="Hơi nghẹn ngào vì một câu chuyện buồn." />
    <TextStringDefinition InstanceID="0xDA92038B" TextString="...vì tất cả những câu chuyện cười." />
    <TextStringDefinition InstanceID="0x6B8F04DB" TextString="Đọc nghệ thuật" />
    <TextStringDefinition InstanceID="0xF15C6B74" TextString="Một số cuốn sách đơn giản là truyền cảm hứng sáng tạo." />
    <TextStringDefinition InstanceID="0x601A5181" TextString="Đọc thú vị" />
    <TextStringDefinition InstanceID="0xEE3B48FE" TextString="Câu chuyện đó thật thú vị, quyến rũ và tươi mới." />
    <TextStringDefinition InstanceID="0xE83563B2" TextString="Đọc logic" />
    <TextStringDefinition InstanceID="0x040E67E3" TextString="Tất cả đều rất hợp lý!" />
    <TextStringDefinition InstanceID="0xFB495BF3" TextString="Đọc trao quyền năng" />
    <TextStringDefinition InstanceID="0x86ACA01C" TextString="Sức mạnh của từ ngữ!" />
    <TextStringDefinition InstanceID="0x44D80E6C" TextString="Đọc nhàm chán" />
    <TextStringDefinition InstanceID="0x5F4B8709" TextString="Việc đọc cuốn sách đó thực sự là một cuộc ngủ trưa dài." />
    <TextStringDefinition InstanceID="0xCFCDE07A" TextString="Đọc gây khó chịu" />
    <TextStringDefinition InstanceID="0xB528E29B" TextString="Tức giận sau khi đọc một bài báo có quan điểm." />
