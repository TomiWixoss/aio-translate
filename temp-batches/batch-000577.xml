    <TextStringDefinition InstanceID="0x6E638F5C" TextString="Canasta" />
    <TextStringDefinition InstanceID="0x79AE92F8" TextString="Sẽ là bất lịch sự nếu lấy một miếng Bánh sinh nhật trước khi nến đã được thổi tắt." />
    <TextStringDefinition InstanceID="0x01CE83C2" TextString="Một số cuốn sách khiến tâm trí độc giả đi thẳng xuống cống." />
    <TextStringDefinition InstanceID="0x905F5D02" TextString="Tư duế bẩn thỉu" />
    <TextStringDefinition InstanceID="0x42C29B1C" TextString="Giờ nghiệp dư" />
    <TextStringDefinition InstanceID="0x4FB18DC0" TextString="Cảm thấy vui vẻ sau khi nghe những câu chuyện cười của các Sim khác!" />
    <TextStringDefinition InstanceID="0xFEE74A87" TextString="Vui vẻ" />
    <TextStringDefinition InstanceID="0xB40BB59F" TextString="Bị từ chối!" />
    <TextStringDefinition InstanceID="0x38F51D72" TextString="Cảm thấy chán nản sau khi bị một Sim khác từ chối!" />
    <TextStringDefinition InstanceID="0xFDE79F57" TextString="Cảm thấy quyến rũ sau khi chỉnh sửa trước gương!" />
    <TextStringDefinition InstanceID="0xEE5CEB3C" TextString="Sự hiện diện quyến rũ" />
    <TextStringDefinition InstanceID="0x9F577740" TextString="Được củng cố sự tự tin sau một trận đấu dữ dội, không hề hấn hại!" />
